target 'ACF' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ACF
  pod 'Alamofire'

  target 'ACFTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
  end

  target 'ACFUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
  end

end
